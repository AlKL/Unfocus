{
    "manifest_version": 2,
    "name": "Unfocus",
    "version": "0.1.0",
    "description": "Break from studying with short Youtube clips",
    "key": "",
    "background": {
        "scripts": [
            "./background.js",
            "./jsrsasign-all-min.js"
        ]
    },
    "browser_action": {
        "default_popup": "./dist/popup.html"
    },
    "options_page": "./options.html",
    "permissions": [
        "identity",
        "<all_urls>", 
        "*://apis.google.com/*",
        "storage"
    ],
    "content_security_policy": "script-src 'self' https://apis.google.com/js/api.js; object-src 'self'"
} 